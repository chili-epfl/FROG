{
	"plugins": [
		"@babel/plugin-transform-flow-strip-types",
		["@babel/plugin-proposal-class-properties", { "loose": true }],
		"@babel/plugin-proposal-decorators",

		"@babel/plugin-proposal-optional-chaining",
		[
			"captains-log",
		{
			"methods": [
				"debug",
			"error",
			"exception",
			"log",
			"warn"
			]
		}
		]
	]
	
}
