{
  "plugins": [
    "transform-optional-chaining",
    "syntax-decorators",
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-flow-strip-types",
    [
      "captains-log",
      {
        "methods": [
          "debug",
          "error",
          "exception",
          "log",
          "warn"
        ]
      }
    ]
  ],
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    "@babel/preset-stage-0",
  ]
}
