{
  "name": "ac-ck-board",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-react,babel-preset-stage-0,babel-preset-es2015 --plugins babel-plugin-transform-class-properties,syntax-flow --out-dir dist && flow-copy-source src dist",
